    "main": "built/main.js",
    "scripts": {
        "build": "npm run lint && tsc -p .",
        "lint": "tsc --noEmit && eslint '*/**/*.ts' --quiet --fix",
        "test": "jest"
    },
    "keywords": [
        "datacoop",
        "version",
        "application"
    ],
    "author": "Evan Higgins <evdhiggins@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/evdhiggins/datacoop-application-versioning.git"
    },
    "license": "ISC",
    "dependencies": {
        "phin": "^3.4.1",
        "prompts": "^2.3.0",
        "typescript": "^3.7.3"
    },
    "devDependencies": {
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.17",
        "@types/phin": "^3.4.0",
        "@types/prompts": "^2.0.3",
        "@typescript-eslint/eslint-plugin": "^2.11.0",
        "@typescript-eslint/parser": "^2.11.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
